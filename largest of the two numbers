import java.util.Scanner;
public class Main{
    public static void main(String[] args)
    {
        int a;
        int b;
        Scanner sc= new Scanner(System.in);
        System.out.println("a= ");
        a=sc.nextInt();
        System.out.println("b= ");
        b=sc.nextInt();
        
        
        
        if(a > b)
        {
            System.out.println("A is Greater ");
        }
        else
         {
            System.out.println("B is Greater");
        }
    }
}
