import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        int n;
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter the value of n : ");
        n=sc.nextInt();
        while(n>0)
        {
            System.out.println(n%10);
            n=n/10;
        }
    }
}
